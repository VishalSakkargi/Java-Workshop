package com.programe01;

class Pen {
	String Color;
	String type;
	
	public void write() {
		System.out.println("writing something");
	}
	
	public void printInfo() {
		System.out.println("this.color");
	}
	
}

public class Classes01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 Pen pen1 = new Pen();
		 Pen pen2 = new Pen();
		 
		 
		 pen1.Color ="blue";
		// pen1.type ="gel";
		 
		 pen1.write();
	}

}
